{
  "name": "gadacz-print",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cp index.dev.html index.html && vite",
    "build": "npm run build:react && npm run build:docs && npm run move:docs",
    "build:react": "cp index.prod.html index.html && tsc -b && vite build",
    "build:docs": "cd docs && npm run build",
    "move:docs": "cp -r ./docs/doc_build ./dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-i18next-types": "npx i18next-resources-for-ts interface -i ./public/locales/en -o ./src/@types/resources.d.ts"
  },
  "dependencies": {
    "@types/react-color": "^3.0.13",
    "clsx": "^2.1.1",
    "csstype": "^3.1.3",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.1",
    "konva": "^9.3.22",
    "overlayscrollbars-react": "^0.5.6",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-konva": "^19.0.7",
    "react-konva-utils": "^1.1.1",
    "use-image": "^1.1.4",
    "zundo": "^2.3.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.4"
  }
}
